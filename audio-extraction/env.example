# Environment configuration
NODE_ENV=production

# Server configuration
PORT=3001

# Backend URL (xtract-backend deployed on Vercel)
XTRACT_BACKEND_URL=https://your-vercel-app.vercel.app

# CORS origins (comma-separated)
ALLOWED_ORIGINS=https://your-vercel-app.vercel.app,https://your-frontend-domain.com

# File storage configuration
TEMP_DIR=./temp
MAX_FILE_SIZE=100

# Cleanup configuration (in hours)
CLEANUP_INTERVAL_HOURS=1
FILE_RETENTION_HOURS=2

# Supabase configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-supabase-anon-key 