{
  "name": "xtract-audio-extraction",
  "version": "2.0.0",
  "description": "Audio extraction service for Xtract - processes video files from Supabase storage",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "clean": "rimraf dist",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.50.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "fluent-ffmpeg": "^2.1.3",
    "node-cron": "^3.0.3",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/fluent-ffmpeg": "^2.1.26",
    "@types/node": "^22.10.0",
    "@types/node-cron": "^3.0.11",
    "@types/uuid": "^10.0.0",
    "rimraf": "^6.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
